{
  "name": "end-to-end",
  "version": "0.0.0",
  "private": true,
  "main": "dest/index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "run tsc -b tsconfig.dest.json",
    "test": "./scripts/start_e2e.sh",
    "formatting": "run -T prettier --check ./src && run -T eslint --max-warnings 0 ./src",
    "clean": "rm -rf ./data"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "globals": {
      "ts-jest": {
        "diagnostics": false,
        "tsconfig": "tsconfig.json",
        "useESM": true
      }
    },
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "testRegex": "./src/.*\\.test\\.ts$",
    "rootDir": "./src"
  },
  "dependencies": {
    "@aztec/barretenberg": "workspace:^",
    "@aztec/blockchain": "workspace:^",
    "@aztec/eslint-config": "workspace:^",
    "@aztec/sdk": "workspace:^",
    "@jest/globals": "^28.1.3",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/debug": "^4.1.7",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.23",
    "debug": "^4.3.4",
    "jest": "^28.1.3",
    "puppeteer": "^21.3.4",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
