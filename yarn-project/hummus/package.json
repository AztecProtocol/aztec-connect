{
  "name": "hummus",
  "version": "1.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "yarn clean && webpack",
    "start": "serve -p 8080 ./dest",
    "start:dev": "webpack serve --mode=development",
    "build:cli": "yarn clean && tsc -p tsconfig.cli.json",
    "start:cli": "node ./dest/cli.js",
    "start:cli:dev": "tsc-watch -p tsconfig.cli.json --onSuccess 'yarn start:cli'",
    "test": "tsc --noEmit",
    "formatting": "run -T prettier --check ./src && run -T eslint --max-warnings 0 ./src",
    "clean": "rm -rf ./dest"
  },
  "browserslist": [
    "last 1 chrome version"
  ],
  "dependencies": {
    "serve": "^14.0.1"
  },
  "devDependencies": {
    "@aztec/eslint-config": "workspace:^",
    "@aztec/sdk": "workspace:^",
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.7.23",
    "@types/react": "18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "browserify-cipher": "^1.0.1",
    "buffer": "^6.0.3",
    "chalk": "^5.0.1",
    "copy-to-clipboard": "^3.3.1",
    "copy-webpack-plugin": "^11.0.0",
    "debug": "^4.3.4",
    "events": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "resolve-typescript-plugin": "^1.2.0",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "styled-components": "^5.3.5",
    "ts-loader": "^9.3.1",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4",
    "util": "^0.12.4",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
