{
  "name": "@aztec/barretenberg",
  "version": "2.0.0",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.dest.json && parcel build ./src/wasm/worker.ts --no-source-maps --out-dir ./dest && node ./package.js",
    "build:dev": "tsc -p tsconfig.dest.json --watch",
    "clean": "rm -rf ./dest ./.cache",
    "test": "jest",
    "symlink-wasm": "cp -R ./src/wasm/barretenberg.wasm ./dest/wasm"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "rootDir": "./src",
    "testTimeout": 30000,
    "slowTestThreshold": 60
  },
  "browserslist": [
    "since 2017-06"
  ],
  "dependencies": {
    "@types/detect-node": "^2.0.0",
    "@types/levelup": "^4.3.0",
    "buffer-browserify": "^0.2.5",
    "debug": "^4.1.1",
    "detect-node": "^2.0.4",
    "fs-extra": "10.0.0",
    "is-node": "^1.0.2",
    "levelup": "^5.0.0",
    "memdown": "^6.0.0",
    "node-fetch": "2.6.1",
    "promise-readable": "^5.0.4",
    "sha3": "^2.1.3",
    "threads": "1.7.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "9.0.11",
    "@types/jest": "^26.0.20",
    "@types/memdown": "^3.0.0",
    "@types/node": "^15.6.2",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "jest": "^27.0.3",
    "parcel-bundler": "^1.12.5",
    "prettier": "^2.0.5",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.4.4"
  }
}
