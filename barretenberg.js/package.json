{
  "name": "barretenberg",
  "version": "1.0.0",
  "main": "index.js",
  "license": "GPL-2.0",
  "scripts": {
    "build": "yarn clean && tsc -p tsconfig.cjs.json && tsc -p tsconfig.es.json && node ./package.js",
    "clean": "rm -rf ./dest ./dest-es",
    "test": "jest",
    "build:dev": "yarn build:dev:cjs & yarn build:dev:es",
    "build:dev:cjs": "tsc -p tsconfig.cjs.json --watch",
    "build:dev:es": "tsc -p tsconfig.es.json --watch",
    "symlink-wasm": "cp -R ./src/wasm/barretenberg.wasm ./dest/wasm && cp -R ./src/wasm/barretenberg.wasm ./dest-es/wasm"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "dependencies": {
    "@types/levelup": "^4.3.0",
    "bigint-buffer": "^1.1.5",
    "debug": "^4.1.1",
    "detect-node": "^2.0.4",
    "levelup": "^4.4.0",
    "memdown": "^5.1.0",
    "node-fetch": "^2.6.0",
    "sha3": "^2.1.3",
    "threads": "1.3.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.1.3",
    "@types/memdown": "^3.0.0",
    "@types/node": "^13.7.7",
    "jest": "^25.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
